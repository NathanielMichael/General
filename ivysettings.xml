<ivy-settings>
  <settings defaultResolver="main" />
  <resolvers>
    <chain name="main">
      <url name="bukkit" m2compatible="true">
        <artifact pattern="http://repo.bukkit.org/artifactory/libs-snapshot-local/[organization]/[module]/[revision]/[module](-[classifier])-[revision].[ext]" />
        <ivy pattern="http://repo.bukkit.org/artifactory/libs-snapshot-local/[organization]/[module]/[revision]/ivy-[revision].xml" />
      </url>
      <ibiblio name="ibiblio" m2compatible="true" />
    </chain>
  </resolvers>
</ivy-settings>